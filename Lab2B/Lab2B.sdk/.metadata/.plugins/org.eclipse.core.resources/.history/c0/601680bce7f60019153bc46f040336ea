#include "mb_interface.h"
#include <xbasic_types.h>
#include <xio.h>


int main()
{
	static XIntc intc;
	static XTmrCtr axi_timer_0_Timer;
	static XTmrCtr axi_timer_1_Timer;
	static XGpio axi_gpio_btn_Gpio;

	Xil_ICacheEnable();
	Xil_DCacheEnable();

		print("---Entering main---\n\r");
    print("Hello World\n\r");

    return 0;
}
